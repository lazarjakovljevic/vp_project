<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>

	<system.serviceModel>
		<client>
			<endpoint name="SensorService"
                      address="net.tcp://localhost:4000/SensorService"
                      binding="netTcpBinding"
                      contract="Common.ISensorService" />
		</client>
	</system.serviceModel>

	<appSettings>
		<add key="csvPath" value="dataset.csv" />
		<add key="maxRows" value="100" />
	</appSettings>
</configuration>